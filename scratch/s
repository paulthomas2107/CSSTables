for (var x = 0; x < 20; x++) {
              
            
              $("#myTable > tbody").append("<tr>");
              $("#myTable > tbody").append("<td>"+people[x].name.first+"</td>");
              $("#myTable > tbody").append("<td>"+people[x].name.last+"</td>");
              $("#myTable > tbody").append("<td>"+people[x].login.uuid+"</td>");
